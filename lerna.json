{
  "version": "0.0.5",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "create": {
      "homepage": "https://github.com/willin/uua",
      "license": "Apache-2.0",
      "access": "public"
    },
    "version": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "message": "release(core): publish %s"
    }
  },
  "ignoreChanges": [
    "**/test/**",
    "**/*.md",
    "packages/docs/**"
  ]
}
